                                    },
                                    error: function () { }
                                });


                            }
                            else {
                                swal({
                                    title: "Opps!",
                                    text: "Cannot remove from cart, please try again.",
                                    icon: "error",
                                });
                            }

                        },
                        error: function () {
                            swal({

                                title: "Opps!",
                                text: "Something went wrong",
                                icon: "error",
                            });
                        }
                    });
                }
            }


    </script>

</body>
</html>