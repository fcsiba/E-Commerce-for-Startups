            function SizeToInt(param) {
                if (param === 'S') {
                    return 1;
                }
                else if (param === 'M') {
                    return 2;
                }
                else if (param === 'L') {
                    return 3;
                }
                else if (param === 'XL') {
                    return 4;
                }
            }

            function SizeToString(param) {
                if (param === 1) {
                    return 'S';
                }
                else if (param === 2) {
                    return 'M'
                }
                else if (param === 3) {
                    return 'L';
                }
                else if (param === 4) {
                    return 'XL';
                }
            }

